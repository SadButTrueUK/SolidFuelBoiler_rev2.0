650:               _OS_Temp = (OS_STIMERS >> 3) + 1;
651:               do
652:               {
653:                   if (_indf)
654:                   {
655:                       if (!(bCreate & 1)) return 0;
656:           
657:                       _asm
658:                           movlw   _OS_StimersFree
659:                           subwf   _fsr, 0, 0
660:                           rlcf    _wreg, 1, 0
661:                           rlcf    _wreg, 1, 0
662:                           rlcf    _wreg, 0, 0
663:                           andlw   0xF8
664:           
665:                           movff   _indf, _OS_TempH  // Search for first "1" in _indf
666:                           movwf   _OS_Temp, 0
667:                           movlw   1
668:                        OS_ST_GF_REPEAT:
669:                           rrcf    _OS_TempH, 1, 0
670:                           bc      OS_ST_GF_OK
671:                           rlcf    _wreg, 1, 0
672:                           incf    _OS_Temp, 1, 0
673:                           bra     OS_ST_GF_REPEAT
674:                        OS_ST_GF_OK:
675:           
676:                           xorwf  _indf, 1, 0         // Clear bit of selected timer
677:           
678:                       _endasm
679:                       return _OS_Temp;
680:           
681:                   }
682:                   _fsr++;
683:               } while (--_OS_Temp);
684:           
685:               _OS_Flags.bError = 1;
686:               return 0xFF;
